    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "handlebars": "^4.0.11",
    "lodash.camelcase": "^4.3.0",
    "mime": "^2.3.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-tdmalone": "^0.1.3",
    "eslint-plugin-dollar-sign": "^1.0.1",
    "eslint-plugin-jest": "^21.23.0",
    "jest": "^23.4.2",
